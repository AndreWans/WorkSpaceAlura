
public class Administrador extends Funcionario implements Altenticavel{

	private int senha;
	
	@Override
	public double getBonificacao() {
		return 50;
	}

	
	@Override
	public boolean autentica(int senha) {
		return this.senha == senha;
	}

	@Override
	public void setSenha(int senha) {
		this.senha = senha;
	}
}
