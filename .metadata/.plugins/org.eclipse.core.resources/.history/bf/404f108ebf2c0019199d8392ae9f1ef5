package Test;

public class TestaConta {

	public static void main(String[] args) {
		Model.ContaCorrente contaCorrente = new Model.ContaCorrente(1, 111);
		contaCorrente.deposita(100.0);
		
		Model.ContaPoupanca contaPoupanca = new Model.ContaPoupanca(1, 222);
		contaPoupanca.deposita(200.0);
		
		try{
			contaCorrente.transfere(10.0, contaPoupanca);
		}catch (Exception e) {
			System.out.println("Exception: " + e.getMessage());
		}
		System.out.println("Conta Corrente: " + contaCorrente.GetSaldo());
		System.out.println("Conta Poupança: " + contaPoupanca.GetSaldo());

	}

}
