
public class Gerente extends Funcionario{

	private int senha;

	public int getSenha() {
		return senha;
	}

	public void setSenha(int senha) {
		this.senha = senha;
	}
	
	public boolean autendica(int senha){
		return (this.senha == senha);
	}
	
//	public double getBonificacao(){
//		return this.salario;
//	}
}
